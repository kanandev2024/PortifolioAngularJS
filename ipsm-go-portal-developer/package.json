{
  "name": "IPSM-PORTAL",
  "description": "IPSM-PORTAL",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "run-p start:dev watch:css",
    "build": "npm run build:css && sapper build --legacy",
    "watch:css": "postcss static/global.css -o static/index.css -w",
    "build:css": "NODE_ENV=production postcss static/global.css -o static/index.css",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "start:dev": "sapper dev",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@smui/data-table": "^2.0.0-beta.1",
    "@tailwindcss/forms": "^0.2.1",
    "compression": "^1.7.1",
    "dayjs": "^1.10.4",
    "npm-run-all": "^4.1.5",
    "polka": "next",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.1",
    "postcss-nested": "^5.0.3",
    "sirv": "^1.0.0",
    "svelte-input-mask": "^1.1.0",
    "svelte-preprocess": "^4.7.0",
    "tailwindcss": "^2.0.3",
    "toastify-js": "^1.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "attractions": "^3.2.0",
    "postcss": "^8.2.9",
    "rollup": "^2.47.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.29.0",
    "sass": "^1.32.8",
    "svelte": "^3.17.3"
  }
}
